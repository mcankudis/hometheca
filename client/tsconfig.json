{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "outDir": "./dist",
        "resolveJsonModule": true,
        "strict": true,
        "target": "ESNext",
        "types": ["vite/client"],
        "isolatedModules": true,
        "paths": {
            "@assets": ["./src/assets"],
            "@login": ["./src/login"],
            "@navigation": ["./src/navigation"]
        }
    }
}
